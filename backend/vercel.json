{
    "version": 2,
    "builds": [
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "https://durian-pay-task.vercel.app/api/search",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
            "Access-Control-Allow-Origin": "*"
                
            }
        }
    ]
}
